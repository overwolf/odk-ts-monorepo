
{
	"name": "odk-ts",
	"entryPoints": ["../../src/window/*.*"],
	"tsconfig": "../../tsconfig.json",
	"out": "../output",
	"cleanOutputDir": true,
	"plugin": ["typedoc-plugin-markdown"],
	"readme": "../../README.md",
	"mergeReadme": true,
	"exclude": ["**/__tests__/**", "**/*.spec.ts", "node_modules"],
	"excludePrivate": true,
	"excludeProtected": false,
	"excludeExternals": true,
	"disableSources": true,
	"hideGenerator": true,
	"includeVersion": false,
	"hidePageHeader": true,
	"hideBreadcrumbs": true,
	"theme": "default",
	"outputFileStrategy": "members",
	"includeHierarchySummary": true  
}

